# Example env for the printmechecks server
PORT=3000
POSTGRID_API_KEY=
POSTGRID_API_URL=
# If POSTGRID_API_KEY is empty, the server will simulate PostGrid responses for local dev

# PostGrid send mode. Values: pdf | raw | auto
POSTGRID_SEND_MODE=auto
# Does the configured PostGrid API accept raw check data? true|false
POSTGRID_API_SUPPORTS_RAW=false
# PostGrid webhook secret (if provided by PostGrid) for verifying callbacks
POSTGRID_WEBHOOK_SECRET=

DATABASE_URL=postgresql://postgres:postgres@postgres:5432/printmechecks?schema=public
AZURE_AD_ISSUER=
AZURE_AD_AUDIENCE=
AZURE_STORAGE_CONNECTION_STRING=
AZURE_BLOB_CONTAINER=printmechecks
ACS_CONNECTION_STRING=
AZURE_STORAGE_ACCOUNT_NAME=
AZURE_CLIENT_ID=
AZURE_TENANT_ID=
AZURE_CLIENT_SECRET=
ACS_ENDPOINT=
ACS_SENDER_ADDRESS=
FILE_MAX_SIZE_BYTES=10485760
